# Copy this file to .env and fill in secrets for local development.

# API keys
SPIDER_KEY=
OPENROUTER_API_KEY=
FETCHFOX_API_KEY=

# Temporal / Convex configuration
# Address of Temporal Frontend (use 127.0.0.1 for Podman on Windows)
TEMPORAL_ADDRESS=127.0.0.1:7233
# Temporal namespace to use
TEMPORAL_NAMESPACE=default
# Task queue for the scraper worker
TEMPORAL_TASK_QUEUE=scraper-task-queue

# Convex HTTP base URL (use .convex.site domain for HTTP router)
CONVEX_HTTP_URL=https://<your-deployment>.convex.site

# Registry / packaging (optional)
GHCR_USERNAME=
GHCR_PAT=

